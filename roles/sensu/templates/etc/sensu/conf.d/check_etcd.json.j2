{
  "checks": {
    "etcd-http-status": {
      "command": "/usr/local/bin/check-etcd.rb -h $(hostname) -p {{ etcd_port }} --cert /etc/etcd/peer.crt --key /etc/etcd/peer.key  --ssl --insecure",
      "subscribers": [
        "etcd"
      ],
      "handlers": ["mailer", "flowdock"],
      "occurrences": 3,
      "refresh": 86400,
      "interval": 60
    }
  }
}
