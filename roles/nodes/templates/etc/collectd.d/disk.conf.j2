LoadPlugin disk

<Plugin disk>
    #Disk "/^xvd[a-f][0-9]?$/"
    #root fs
    Disk "/{{ collectd_disk_rootfs }}/"
    #volume docker storage
    Disk "/{{ docker_storage_dev }}1/"
{% if inventory_hostname in groups['masters'] and collectd_disk_ose_registry %}
    #volume ose-registry
    Disk "/{{ collectd_disk_ose_registry }}/"
{% endif %}
    IgnoreSelected false
</Plugin>
